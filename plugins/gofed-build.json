{"name": "gofed-scan",
"commands": [
	{"command": "bbo", "help": "buildroot override builds for all branches", "script": "bbobranches --bbo"},
	{"command": "build", "help": "build all fedora branches", "script": "build --build"},
	{"command": "bump", "help": "bump spec file", "script": "bumpSpec.py"},
	{"command": "gcpmaster", "help": "git cherry pick master branch", "script": "gcp --gcp"},
	{"command": "pull", "help": "pull from branches", "script": "pull --pull"},
	{"command": "push", "help": "push to branches", "script": "push --push"},
	{"command": "scratch-build", "help": "scratch build all fedora branches", "script": "scratch-build --scratch"},
	{"command": "update", "help": "update all fedora branches", "script": "update --update"},
	{"command": "wizard", "help": "run phases of bulding, updating, ... at once", "script": "wizard.py"}
]}
